- var chat_menu             = true;
- var pe7_icon              = true;
- var button_builder        = true;

doctype html
html(lang='en')
    include ../../components/header-files
    body.button-builder
        include ../../components/loader
        // page-wrapper Start
        .page-wrapper
            include ../../components/header
            // Page Body Start
            .page-body-wrapper
                include ../../components/sidebar
                include ../../components/chat-sidebar
                .page-body
                    .container-fluid
                        .page-header
                            .row
                                .col-lg-6.main-header
                                    h2
                                      | Button
                                      span
                                        | Builder
                                    h6.mb-0 admin panel
                                .col-lg-6.breadcrumb-right      
                                    ol.breadcrumb
                                      li.breadcrumb-item
                                        a(href='index.html')
                                         i.pe-7s-home
                                      li.breadcrumb-item Perk UI
                                      li.breadcrumb-item Builders  
                                      li.breadcrumb-item.active Button Builder
                    // Container-fluid starts
                    .container-fluid
                      .row
                        .col-sm-12
                          .card
                            .card-header
                              h5 Button Builder
                            .card-body
                              .button-builder-wrap
                                .row
                                  .col-md-6.col-lg-7.order-2.order-xl-1
                                    .form-horizontal.generator.theme-form
                                      .form-group.row
                                        label.col.col-form-label Buttons Text
                                        .col-lg-9
                                          input.form-control.button-text(type='text', value='Button')
                                      .form-group.row
                                        label.col.col-form-label Element Type
                                        .col-lg-9
                                          .element-type.grp-btns(data-toggle='buttons')
                                            label.btn.btn-light.active
                                              input(type='radio', name='type', value='a')
                                              |  Anchor
                                            label.btn.btn-light
                                              input(type='radio', name='type', value='button')
                                              |  Button
                                            label.btn.btn-light
                                              input(type='radio', name='type', value='input')
                                              |  Input
                                    .form-group.row
                                      label.col.col-form-label Button Size
                                      .col-lg-9
                                        .button-size.grp-btns(data-toggle='buttons')
                                          label.btn.btn-light.active
                                            input(type='radio', name='size', value='btn-lg')
                                            |  Large
                                          label.btn.btn-light
                                            input(type='radio', name='size', value='')
                                            |  Default
                                          label.btn.btn-light
                                            input(type='radio', name='size', value='btn-sm')
                                            |  Mini
                                    .form-group.row
                                      label.col.col-form-label.button-style Button Style
                                      .col-lg-9
                                        .button-color-option
                                          label.mr-3
                                            input(type='radio', name='color-option', checked='checked', value='bootstrap')
                                            |  Bootstrap Inbuilt
                                          label
                                            input(type='radio', name='color-option', value='custom')
                                            |  Custom
                                    .bootstrap-inbuilt-style
                                      .form-group.row
                                        label.col.col-form-label Button Type
                                        .col-lg-9
                                          .button-color.grp-btns(data-toggle='buttons')
                                            label.btn.btn-primary
                                              input(type='radio', name='color', value='btn btn-primary')
                                              |  Primary
                                            label.btn.btn-secondary
                                              input(type='radio', name='color', value='btn btn-secondary')
                                              |  Seconadary
                                            label.btn.btn-success.active
                                              input(type='radio', name='color', value='btn btn-success')
                                              |  Success
                                            label.btn.btn-info
                                              input(type='radio', name='color', value='btn btn-info')
                                              |  Info
                                            label.btn.btn-warning
                                              input(type='radio', name='color', value='btn btn-warning')
                                              |  Warning
                                            label.btn.btn-danger
                                              input(type='radio', name='color', value='btn btn-danger')
                                              |  Danger
                                            label.btn.btn-light
                                              input(type='radio', name='color', value='btn btn-light')
                                              |  Light
                                      .form-group.row
                                        label.col.col-form-label Button State
                                        .col-lg-9
                                          .button-state.grp-btns(data-toggle='buttons')
                                            label.btn.btn-light.active
                                              input(type='radio', name='state', value='normal')
                                              |  Normal
                                            label.btn.btn-light
                                              input(type='radio', name='state', value='active')
                                              |  Active
                                            label.btn.btn-light
                                              input(type='radio', name='state', value='disabled')
                                              |  Disabled
                                    .form-group.row
                                      label.col.col-form-label Button Width
                                      .col-lg-9
                                        .button-width.grp-btns(data-toggle='buttons')
                                          label.btn.btn-light.active
                                            input(type='radio', name='width', value='')
                                            |  Auto
                                          label.btn.btn-light
                                            input(type='radio', name='width', value='btn-block')
                                            |  Full Width
                                    .custom-button-color
                                      .form-group.row
                                        label.col.col-form-label(for='textColor') Text & Icon Color
                                        .col-lg-9
                                          input#textColor.form-control.input-sm.pull-left(type='text', maxlength='6', size='6')
                                          #colorSelector1.color-slelector
                                            div
                                      .form-group.row
                                        label.col.col-form-label(for='topColor') Gradient Top Color
                                        .col-lg-9
                                          input#topColor.form-control.input-sm.pull-left(type='text', maxlength='6', size='6')
                                          #colorSelector2.color-slelector
                                            div
                                      .form-group.row
                                        label.col.col-form-label(for='bottomColor') Gradient Bottom Color
                                        .col-lg-9
                                          input#bottomColor.form-control.input-sm.pull-left(type='text', maxlength='6', size='6')
                                          #colorSelector3.color-slelector
                                            div
                                      .form-group.row
                                        label.col.col-form-label(for='bgColor') Background Color
                                        .col-lg-9
                                          input#bgColor.form-control.input-sm.pull-left(type='text', maxlength='6', size='6')
                                          #colorSelector4.color-slelector
                                            div
                                      .form-group.row
                                        label.col.col-form-label(for='borderTopColor') Border Top Color
                                        .col-lg-9
                                          input#borderTopColor.form-control.input-sm.pull-left(type='text', maxlength='6', size='6')
                                          #colorSelector5.color-slelector
                                            div
                                          label.hint-label.border-all
                                            input(type='checkbox', name='border-all')
                                            |  Use same color for all sides
                                      .border-color-separate
                                        .form-group.row
                                          label.col.col-form-label(for='borderHrColor') Border Left & Right Color
                                          .col-lg-9
                                            input#borderHrColor.form-control.input-sm.pull-left(type='text', maxlength='6', size='6')
                                            #colorSelector6.color-slelector
                                              div
                                        .form-group.row
                                          label.col.col-form-label(for='borderBottomColor') Border Bottom Color
                                          .col-lg-9
                                            input#borderBottomColor.form-control.input-sm.pull-left(type='text', maxlength='6', size='6')
                                            #colorSelector7.color-slelector
                                              div
                                    #iconOption.form-group.row
                                      label.col.col-form-label Icon
                                      .col-lg-9
                                        .include-icon.grp-btns(data-toggle='buttons')
                                          label.btn.btn-light.active
                                            input(type='radio', name='includeicon', value='1')
                                            span.fa.fa-check
                                          label.btn.btn-light
                                            input(type='radio', name='includeicon', value='0')
                                            span.fa.fa-times
                                    .form-group.row.toggle
                                      label.col.col-form-label Icon Position
                                      .col-lg-9
                                        .fa-position.grp-btns(data-toggle='buttons')
                                          label.btn.btn-light.active
                                            input(type='radio', name='iconposition', value='left')
                                            span.fa.fa-arrow-left
                                            |  Left
                                          label.btn.btn-light
                                            input(type='radio', name='iconposition', value='right')
                                            |  Right 
                                            span.fa.fa-arrow-right
